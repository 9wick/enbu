{
  "name": "enbu-vscode",
  "displayName": "Enbu - Browser Automation Visualizer",
  "description": "Run and visualize enbu.yaml flow tests with VS Code Test Explorer",
  "version": "0.0.1",
  "publisher": "agent-browser-flow",
  "type": "module",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [
    "workspaceContains:**/*.enbu.yaml"
  ],
  "main": "./dist/extension.mjs",
  "contributes": {},
  "scripts": {
    "build": "nx run vscode-extension:build",
    "dev": "nx run vscode-extension:dev",
    "typecheck": "nx run vscode-extension:typecheck",
    "lint": "nx run vscode-extension:lint",
    "lint:check": "nx run vscode-extension:lint --configuration=ci"
  },
  "dependencies": {
    "@packages/agent-browser-adapter": "workspace:*",
    "@packages/core": "workspace:*",
    "neverthrow": "^8.2.0",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@packages/config": "workspace:*",
    "@types/node": "^22.14.1",
    "@types/vscode": "^1.85.0"
  }
}
